class Solution {
    public int[] searchRange(int[] nums, int target) {
        int res[]=new int[2];
        int l=0,r=nums.length-1;
        while(l<=r)
        {
            if(nums[l]==target && nums[r]==target)
            {
                res[0]=l;
                res[1]=r;
                return res;
            }
            if(nums[l]!=target)
            l++;
            if(nums[r]!=target)
            r--;
        }
            return new int[]{-1,-1};
    }        
}



    
